{
  "build": {
    "beforeDevCommand": "",
    "beforeBuildCommand": "",
    "devPath": "../src",
    "distDir": "../src",
    "withGlobalTauri": true
  },
  "package": {
    "productName": "dashboard-products",
    "version": "0.0.0"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "shell": {
        "all": false,
        "open": true
      },
      "dialog": {
        "all": false,
        "ask": false,
        "confirm": true,
        "message": true,
        "open": false,
        "save": false
      },
      "app": {
        "all": true,
        "hide": false,
        "show": false
      }
    },

    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "com.etaelith.dev",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ]
    },

    "security": {
      "csp": null
    },
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "dashboard-products",
        "width": 1920,
        "height": 1080
      }
    ]
  }
}
